CLANG_CXX_LANGUAGE_STANDARD = c++11
CLANG_CXX_LIBRARY = libc++
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/PopMenu" "${PODS_ROOT}/Headers/Public/XHRealTimeBlur" "${PODS_ROOT}/Headers/Public/pop"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/PopMenu" -isystem "${PODS_ROOT}/Headers/Public/XHRealTimeBlur" -isystem "${PODS_ROOT}/Headers/Public/pop"
OTHER_LDFLAGS = $(inherited) -ObjC -l"PopMenu" -l"XHRealTimeBlur" -l"c++" -l"pop" -framework "CoreGraphics" -framework "Foundation" -framework "UIKit"
PODS_ROOT = ${SRCROOT}/Pods